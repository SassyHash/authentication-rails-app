<h1>Sign-Up</h1>
<%= form_for @user do |f| %>
  <%= f.label :username, "Username:" %>
  <%= f.text_field :username %>
  <br>
  <%= f.label :password, "Password:" %>
  <%= f.password_field :password %>
  <br>
  <%= f.submit "Create!" %>
<% end %>
<%= link_to "Already have an account?", new_session_path %>